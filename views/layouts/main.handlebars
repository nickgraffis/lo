  {{{_sections.styles}}}
  <style>
    .tooltip .tooltip-text {
      text-align: center;
      position: absolute;
      z-index: 100;
    }
    .tooltip-text:before {
      content: '';
      position: absolute;
      background-color: inherit;
      width: 28px;
      height: 28px;
      transform: rotate(45deg);
      margin-left: 47%;
      margin-top: -5px;
    }
    #charecters img.emoji {
      height: 1em;
      width: 1em;
      margin: 0 .05em 0 .1em;
      vertical-align: -0.1em;
      display: inline-block;
    }

    #article h4 {

    }

    #article p {
      font-family: 'Noto Serif', serif;
      letter-spacing: -0.003em;
      line-height: 32px;
      font-size: 21px;
      margin-bottom: -0.46em;
      margin-top: 24px;
      color: rgb(41, 41, 41);
      word-break: break-word;
      font-weight: 400;
      display: block;
      margin-block-start: 1em;
      margin-block-end: 1em;
      margin-inline-start: 0px;
      margin-inline-end: 0px;
    }

    #article pre {
      margin-top: 56px;
      overflow-x: scroll;
      background-image: initial;
      background-position-x: initial;
      background-position-y: initial;
      background-size: initial;
      background-attachment: initial;
      background-origin: initial;
      background-clip: initial;
      background-color: rgb(242, 242, 242);
      margin: 12px 0 12px 0;
      display: block;
      font-family: monospace;
      white-space: pre;
      font-weight: 400;
      padding: 20px;
      color: rgb(41, 41, 41);
      font-size: 18px;
    }

    #article code {
      background-color: rgb(242, 242, 242);
      font-family: monospace;
      font-weight: 400;
      padding: 8px;
      color: rgb(41, 41, 41);
      font-size: 18px;
    }
  </style>
  <link href="/css/lo_style.css" rel="stylesheet">
</head>
<body class="h-screen">
  {{{body}}}
  {{> modal }}
  {{{_sections.script}}}
  <script src="/js/lo.js"></script>
</body>

</html>
